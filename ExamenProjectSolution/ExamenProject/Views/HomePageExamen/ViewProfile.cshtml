@model ExamenProject.Models.ExamenDataModel

@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>ViewProfile</title>
</head>
<body>
   

    @using (Html.BeginForm())
    {

        @Html.ValidationSummary(true, "", new { @class = "text-danger" })
        
            <div>
                @ViewBag.Message
            </div>
            <div>
                <table style="border:solid;align-content:center">
                    <tr>
                        <th>
                            TEST ID 
                        </th>
                       
                        <th>
                            TESTMODEL
                        </th>
                        <th>
                            SCORES
                        </th>
                    </tr>
                    <tr>
                        <td>
                            
                            @for (int i = 0; i < Model.Examens.Count; i++)
                            {
                                @Html.DisplayFor(x => x.Examens[i].Candidate_ID)

                            }
                        </td>
                        
                    <td>
                        @for (int i = 0; i < Model.Examens.Count; i++)
                        {
                            @Html.DisplayFor(x => x.Examens[i].Testmodel)

                        }
                    </td>
                        
                    <td>
                        @for (int i = 0; i < Model.Examens.Count; i++)
                        {
                            @Html.DisplayFor(x => x.Examens[i].Mark)

                        }
                    </td>
                        
                    </tr>
                </table>
            </div>
        

    }


</body>
</html>
